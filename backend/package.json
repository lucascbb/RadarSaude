{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/server.js",
    "restart": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "undo": "npx sequelize-cli db:migrate:undo:all",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "create:migration": "npx sequelize-cli migration:create --name",
    "create:seeder": "npx sequelize-cli seed:create --name"
  },
